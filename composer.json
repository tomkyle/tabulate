{
    "name": "tomkyle/tabulate",
    "description": "Format 2D arrays as CLI console table, Markdown, CSV, YAML, JSON.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "table",
        "array",
        "markdown",
        "json",
        "csv",
        "yaml",
        "cli"
      ],
    "require": {
        "php": "^8.3",
        "symfony/yaml": "^7.3",
        "symfony/console": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "tomkyle\\Tabulate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "tomkyle/find-run-test": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.67",
        "rector/rector": "^2.0",
        "phpstan/phpstan": "^2.1"
    }

}
